public class Main
{
	public static void main(String[] args) {
		int []arr={1,2,5,4,9};
		int n=arr.length;
		int count=0;
		for(int i=0;i<n;i++){
		    if(arr[i]%2==0){
		        System.out.println("even:"+arr[i]);
		        count++;
		    }
		    else{
		        System.out.println("odd:"+arr[i]);
		    }
		    
		}
		System.out.println("count:"+count);
		
	}
